@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<br />
<button class="btn btn-info" id="btn1">Button1</button>
<button class="btn btn-danger" id="btn2">Button2</button>
<button class="btn btn-success" id="btn3">Button3</button>


<script>
    $('#btn1').click(function () {
        swal("Merhaba Mvc Proje Kampı");
    });
</script>

<script>
    $('#btn2').click(function () {
        swal("Mvc Proje Kampı!", "...Ders 96");
    });
</script>

<script>
    $('#btn3').click(function () {
        swal({
            title: "Tebrikler!",
            text: "Başarılı bir şekilde eklendi!",
            icon: "success",
        });
    });
</script>